#!/bin/bash

set -ex

# aws s3 cp s3://blog.xaviershay.com-storage/rundata.json.gz ./tmp/
aws s3 sync s3://blog.xaviershay.com-storage/ tmp --exclude '*' --include rundata.json.gz
